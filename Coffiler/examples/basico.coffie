#include <stdio.h>

int main() {
    int x = 10;
    double y = 5.5;
    char c = 'A';

    // Teste de Printf
    printf("Inteiro: %d\n", x);
    printf("double: %f\n", y);
    printf("Char: %c\n", c);

    // Teste de Operadores
    int soma = x + 10;
    float mult = y * 2.0;
    
    printf("Soma (10+10): %d\n", soma);
    printf("Mult (5.5*2): %f\n", mult);

    return 0;
}